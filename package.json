{
  "name": "supafetch",
  "version": "0.1.0",
  "description": "Fetch Wrapper inspired by axios",
  "main": "lib/index.js",
  "repository": "https://github.com/jemmyphan/supafetch.git",
  "scripts": {
    "build:node": "node_modules/.bin/babel src --modules common --out-dir lib",
    "build:web": "webpack --output-filename supafetch.js",
    "build:web.min": "webpack --output-filename supafetch.min.js -p",
    "build": "npm-run-all --parallel build:*",
    "prepublish": "npm run build"
  },
  "keywords": [
    "fetch",
    "axios",
    "network",
    "http-client",
    "http",
    "xhr"
  ],
  "author": "Jemmy Phan <jemmyphan94@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "npm-run-all": "^4.0.2",
    "path": "^0.12.7",
    "webpack": "^3.3.0"
  },
  "dependencies": {
    "qs": "^6.5.0"
  }
}
